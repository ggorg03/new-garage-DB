-- Consultando motos que tem mais de 6 meses desde o ultimo servi√ßo
SELECT mc.id, mc.cliente_id, MAX(s.kilometragem_revisao), MAX(s.data_servico)
FROM Servicos AS s
INNER JOIN Motos_Clientes AS mc ON mc.id = s.motos_clientes_id
WHERE s.data_servico < current_date + interval '6 month'
GROUP BY mc.id;